{
  "name": "culli",
  "version": "0.0.1",
  "private": false,
  "description": "Cycle Utility Libraries for clean, well-structured and concise code",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milankinen/culli.git"
  },
  "keywords": [
    "rxjs",
    "reactive programming",
    "cyclejs",
    "most"
  ],
  "scripts": {
    "postinstall": "npm run init",
    "test:ci": "lerna run test:ci",
    "report:ci": "lerna run report:ci",
    "postreport:ci": "rimraf coverage && mkdir -p coverage && lcov-result-merger 'packages/*/coverage/lcov.info' coverage/lcov.info",
    "build": "lerna run build",
    "init": "lerna bootstrap && lerna run update:binaries",
    "test": "lerna run test",
    "clean": "lerna run clean",
    "update-deps": "ncu -u -a && lerna run update:modules",
    "create-package": "lehm create package"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-register": "^6.16.3",
    "buble": "^0.14.0",
    "codecov": "^1.0.1",
    "eslint": "^3.8.1",
    "lcov-result-merger": "^1.2.0",
    "lehm": "^0.3.4",
    "lerna": "^2.0.0-beta.24",
    "mocha": "^3.1.2",
    "npm-check-updates": "^2.8.5",
    "nyc": "^8.3.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "shelljs": "^0.7.4",
    "uglify-js": "^2.7.3"
  }
}
