{
  "name": "culli",
  "version": "0.0.1",
  "private": false,
  "description": "Cycle Utility Libraries for clean, well-structured and concise code",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": { "type": "git", "url": "git+https://github.com/milankinen/culli.git" },
  "keywords": [ "rxjs", "reactive programming", "cyclejs", "most" ],
  "scripts": {
    "postinstall": "npm run setup && npm run build",
    "test:ci": "nb exec -- npm run test:ci",
    "report:ci": "nb exec -- npm run report:ci",
    "postreport:ci": "rimraf coverage && mkdir -p coverage && lcov-result-merger 'packages/*/coverage/lcov.info' coverage/lcov.info",
    "setup": "nb link && nb exec -- npm i && nb exec -- npm run update:bins",
    "build": "nb exec -- npm run build",
    "test": "nb exec -- npm run test",
    "clean": "nb exec -- npm run clean",
    "postclean": "rm -rf node_modules",
    "upd": "ncu -u -a && nb exec -- npm run update:deps",
    "newpkg": "lehm create package",
    "commit": "northbrook commit",
    "ci": "nb commit",
    "release": "northbrook release"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-register": "^6.16.3",
    "buble": "^0.14.0",
    "codecov": "^1.0.1",
    "eslint": "^3.8.1",
    "ghooks": "^1.3.2",
    "lcov-result-merger": "^1.2.0",
    "lehm": "^0.3.4",
    "mocha": "^3.1.2",
    "northbrook": "^3.2.1",
    "npm-check-updates": "^2.8.5",
    "nyc": "^8.3.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "shelljs": "^0.7.4",
    "uglify-js": "^2.7.3",
    "validate-commit-msg": "^2.8.2"
  },
  "config": {
    "ghooks": { "commit-msg": "node ./node_modules/.bin/validate-commit-msg" },
    "validate-commit-msg": { "types": "@northbrook/commit-types" }
  }
}
